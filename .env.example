# API Keys
OPENAI_API_KEY=your_openai_api_key_here
FEEDLY_ENTERPRISE_TOKEN=your_feedly_enterprise_token_here

# Configuration
TWTW_CONFIG_PATH=config.yaml
TWTW_OUTPUT_DIR=output

# Cache settings
TWTW_CACHE_DIRECTORY=cache
TWTW_CACHE_DURATION_DAYS=7

# Rate limiting
TWTW_RATE_LIMITING_REQUESTS_PER_SECOND=1
TWTW_RATE_LIMITING_MAX_CONCURRENT=5
TWTW_RATE_LIMITING_TIMEOUT_SECONDS=30

# OpenAI settings
TWTW_OPENAI_MODEL=gpt-4
TWTW_OPENAI_TEMPERATURE=0.7 